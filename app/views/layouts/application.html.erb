<!DOCTYPE html>
<html>
<head>
  <title>Jungle</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

      <% puts 'erb: session_id', session[:user_id] %>
      <% if 'cool_method' %>
        <%= 'current_user.first_name' %> | <%= link_to "Logout", "/sessions/#{session[:user_id]}", html_options = {method: :delete} %>
      <% else %>
        <%= link_to 'Login', '/sessions/new' %> | <%= link_to 'Signup', '/users/new' %>
      <% end %>
      <% yield %>

  <%= render 'layouts/top_nav' %>

  <main class="container">
    <%= yield %>
  </main>

  <%= yield :end_of_body %>
</body>
</html>
